{
  "name": "manageos",
  "version": "2.5.3",
  "description": "ManageOS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "publish": "npm run build && npm publish --access=public --ignore-scripts && npm run clean",
    "patch": "node BUILD_TOOLS\\patch.js",
    "build": "BUILD_TOOLS\\build.bat && npm run patch",
    "clean": "BUILD_TOOLS\\clean.bat",
    "postinstall": "node scripts/install-os-deps.js"
  },
  "keywords": [
    "windows",
    "manageos",
    "system"
  ],
  "author": "Tiziano Tomas Luzi Ramos",
  "license": "MIT",
  "files": [
    "dist",
    "README.md",
    "scripts"
  ],
  "dependencies": {
    "ftp-srv": "^4.6.3",
    "loudness": "^0.4.2",
    "node-hid": "^3.2.0",
    "node-mic": "^1.0.1",
    "node-notifier": "^10.0.1",
    "node-webcam": "^0.8.2",
    "node-windows": "^1.0.0-beta.8",
    "ping": "^1.0.0",
    "regedit": "^5.1.4",
    "screenshot-desktop": "^1.15.2",
    "systeminformation": "^5.27.10",
    "win-screen-resolution": "^3.0.2"
  },
  "devDependencies": {
    "@types/node": "^24.9.2",
    "@types/node-notifier": "^8.0.5",
    "@types/node-webcam": "^0.8.3",
    "@types/node-windows": "^0.1.6",
    "@types/screenshot-desktop": "^1.15.0",
    "@types/systeminformation": "^3.23.1",
    "typescript": "^5.6.3"
  }
}
